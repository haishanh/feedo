{
  "name": "feedo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "ava",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*"
